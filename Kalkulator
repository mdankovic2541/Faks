    private void btnPutaActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btnPutaActionPerformed
      prviBroj = Double.parseDouble(txtfZaslon.getText());
      txtfZaslon.setText("");
      operacija = "*";
    }//GEN-LAST:event_btnPutaActionPerformed

