    private void btnJednakoActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btnJednakoActionPerformed
        String odg;
        
        drugiBroj=Double.parseDouble(txtfZaslon.getText());
        if(operacija == "+"){
            rezultat= prviBroj + drugiBroj;
            odg = String.format("%.2f", rezultat);
            txtfZaslon.setText(odg);
        }else if(operacija == "-"){
            rezultat= prviBroj - drugiBroj;
            odg = String.format("%.2f", rezultat);
            txtfZaslon.setText(odg);
        }else if(operacija == "/"){
            rezultat= prviBroj / drugiBroj;
            odg = String.format("%.2f", rezultat);
            txtfZaslon.setText(odg);
        }else if(operacija == "*"){
            rezultat= prviBroj * drugiBroj;
            odg = String.format("%.2f", rezultat);
            txtfZaslon.setText(odg);
        }
    }//GEN-LAST:event_btnJednakoActionPerformed

    private void btnClearActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btnClearActionPerformed
